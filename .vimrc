syntax on
filetype plugin indent on
set termguicolors
set background=dark
set term=xterm
set nocompatible
set clipboard=unnamed
set ruler
set smartindent
set shiftwidth=4
set tabstop=4
set noexpandtab
set backspace=indent,eol,start
set noswapfile
set hlsearch
set ignorecase
set incsearch
set cscopetag
set number relativenumber

nnoremap d "_d
set tags=/ws/gitws/tags
